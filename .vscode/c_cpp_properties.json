{
    "configurations": [
        {
            "name": "STM32",
            "includePath": [
                "C://keil_v5/ARM/ARMCC/**",		//这里修改为相应的keil5文件目录
                "${workspaceFolder}/**"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceRoot}/.vscode/.browse.c_cpp.db",
                "path": [
                    "C://keil_v5/ARM/ARMCC/**",
                    "${workspaceFolder}/**"
                ]
            },
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "__CC_ARM",
                "USE_STDPERIPH_DRIVER",
                "STM32F10X_HD"
            ],
            "intelliSenseMode": "msvc-x64"
        }
    ],
    "version": 4
}